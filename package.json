{
  "dependencies": {
    "@create-figma-plugin/ui": "^2.5.0",
    "@create-figma-plugin/utilities": "^2.5.0",
    "preact": ">=10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^2.5.0",
    "@create-figma-plugin/tsconfig": "^2.5.0",
    "@figma/plugin-typings": "^1.69.0",
    "typescript": ">=4"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "link-spacing-variables",
    "name": "Link spacing variables",
    "menu": [
      {
        "name": "Link spacing variables",
        "main": {
          "src": "src/main.ts",
          "handler": "linkSpacings"
        },
        "ui": "src/ui.tsx"
      },
      "-",
      {
        "name": "Settings",
        "main": {
          "src": "src/main.ts",
          "handler": "settings"
        },
        "ui": "src/ui.tsx"
      }
    ]
  }
}
